            <TextField fx:id="txtNom" layoutX="29.0" layoutY="110.0" prefHeight="51.0" prefWidth="351.0" promptText="Enter Name" />
            <PasswordField fx:id="txtPassword" layoutX="29.0" layoutY="258.0" prefHeight="51.0" prefWidth="351.0" promptText="Enter Password" />
